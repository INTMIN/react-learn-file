{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "cross-env UMI_ENV=dev umi build",
    "dev": "cross-env UMI_ENV=dev umi build",
    "test": "umi test",
    "lint": "eslint {src,mock,tests}/**/*.{js,jsx} --fix",
    "precommit": "lint-staged",
    "analyze": "cross-env ANALYZE=1 umi build"
  },
  "dependencies": {
    "@eslint/eslintrc": "^0.4.2",
    "antd": "^4.5.2",
    "cross-env": "^7.0.2",
    "qs": "^6.9.1",
    "raw-loader": "^4.0.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-id-swiper": "^4.0.0",
    "react-markdown": "^7.1.1",
    "rehype-highlight": "^5.0.2",
    "remark-gfm": "^3.0.1",
    "swiper": "^6.2.0",
    "umi": "^3.5.20",
    "umi-request": "^1.3.4"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.10.4",
    "@umijs/preset-react": "^1.3.0",
    "@babel/eslint-parser": "^7.11.0",
    "eslint": "^7.6.0",
    "eslint-config-umi": "^1.6.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^4.2.5",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.14.0"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --end-of-line=lf --write ",
      "git add"
    ],
    "*.ts?(x)": [
      "prettier --write --end-of-line=lf --parser=typescript ",
      "git add"
    ]
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
